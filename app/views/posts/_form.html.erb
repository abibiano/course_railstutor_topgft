<%= form_for @post do |f| %>
	<%= render 'shared/error_messages', object: @post %>
	
 	<%= f.label :url, "URL" %>
 	<%= f.text_field :url %>

 	<%= f.label :title %>
 	<%= f.text_field :title %>

 	<%= f.label :category_id %>
 	<%= f.collection_select :category_id, Category.all, :id, :name %>

 	<%= f.submit "Save", class: "btn btn-large btn-primary" %>
 <% end %>